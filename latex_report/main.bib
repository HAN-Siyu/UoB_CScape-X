}
@article{BassettCAL1961,
abstract = {Radioactive carbaryl, carbofuran, parathion, leptophos, and DDT were added to cigarettes and the mainstream smoke was directed to the lungs of rats via the trachea. Total radiocarbon transfer to the lungs ranged from 9 to 15{\%} of that in the tobacco burned during a smoking process involving eight 5-ml puffs. Exhalation of 14C residues during this time was 24 to 30{\%} of that inhaled with all insecticides except carbofuran, of which 42{\%} of the residues was exhaled. After 5 hr, total exhalation of the consumed radiocarbon was 35{\%} for parathion, 65{\%} for carbofuran, and approximately 50{\%} for the other products. The nature of the 14C residues inhaled, their urinary and fecal excretion, and their deposition in and dissipation from various organs and tissues are presented.},
