issn = {1664-462X},
journal = {Frontiers in plant science},
keywords = {expression analysis,high-throughput sequencing,microRNA,ncRNAome,non-coding RNA,snoRNA,tRNA},
pages = {708},
pmid = {25566282},
title = {{plantDARIO: web based quantitative and qualitative analysis of small RNA-seq data in plants.}},
