issn = {14774054},
journal = {Briefings in Bioinformatics},
keywords = {Biological network,Complex disease,Computational model,LncRNA-disease association prediction,Long non-coding RNA,Machine learning,biological network,complex disease,computational model,disease association prediction,learning,lncrna,long non-coding rna,machine},
number = {4},
pages = {558--576},
pmid = {27345524},
title = {{Long non-coding RNAs and complex diseases: From experimental results to computational models}},
url = {https://academic.oup.com/bib/article-lookup/doi/10.1093/bib/bbw060},
