pmid = {25707511},
title = {{LncRNA2Function: a comprehensive resource for functional investigation of human lncRNAs based on RNA-seq data.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25707511 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4331805},
