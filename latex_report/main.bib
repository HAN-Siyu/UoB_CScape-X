month = {aug},
number = {16},
pages = {2069--70},
pmid = {20562413},
title = {{Deriving the consequences of genomic variants with the Ensembl API and SNP Effect Predictor.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20562413 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2916720},
