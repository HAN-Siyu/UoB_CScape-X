pmid = {17597888},
title = {{A coding measure scheme employing electron-ion interaction pseudopotential (EIIP).}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1891688{\&}tool=pmcentrez{\&}rendertype=abstract},
