@article{Rocha2014,
abstract = {During X chromosome inactivation (XCI), the Polycomb Repressive Complex 2 (PRC2) is thought to participate in the early maintenance of the inactive state. Although Xist RNA is essential for the recruitment of PRC2 to the X chromosome, the precise mechanism remains unclear. Here, we demonstrate that the PRC2 cofactor Jarid2 is an important mediator of Xist-induced PRC2 targeting. The region containing the conserved B and F repeats of Xist is critical for Jarid2 recruitment via its unique N-terminal domain. Xist-induced Jarid2 recruitment occurs chromosome-wide independently of a functional PRC2 complex, unlike at other parts of the genome, such as CG-rich regions, where Jarid2 and PRC2 binding are interdependent. Conversely, we show that Jarid2 loss prevents efficient PRC2 and H3K27me3 enrichment to Xist-coated chromatin. Jarid2 thus represents an important intermediate between PRC2 and Xist RNA for the initial targeting of the PRC2 complex to the X chromosome during onset of XCI.},
author = {da Rocha, Sim{\~{a}}o Teixeira and Boeva, Valentina and Escamilla-Del-Arenal, Martin and Ancelin, Katia and Granier, Camille and Matias, Neuza Reis and Sanulli, Serena and Chow, Jen and Schulz, Edda and Picard, Christel and Kaneko, Syuzo and Helin, Kristian and Reinberg, Danny and Stewart, A. Francis and Wutz, Anton and Margueron, Rapha{\"{e}}l and Heard, Edith},
doi = {10.1016/j.molcel.2014.01.002},
isbn = {1097-4164 (Electronic)$\backslash$n1097-2765 (Linking)},
issn = {1097-4164},
journal = {Molecular cell},
month = {jan},
number = {2},
pages = {301--16},
pmid = {24462204},
title = {{Jarid2 Is Implicated in the Initial Xist-Induced Targeting of PRC2 to the Inactive X Chromosome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24462204},
volume = {53},
year = {2014}
}
@article{Frith2006,
abstract = {Short proteins play key roles in cell signalling and other processes, but their abundance in the mammalian proteome is unknown. Current catalogues of mammalian proteins exhibit an artefactual discontinuity at a length of 100 aa, so that protein abundance peaks just above this length and falls off sharply below it. To clarify the abundance of short proteins, we identify proteins in the FANTOM collection of mouse cDNAs by analysing synonymous and non-synonymous substitutions with the computer program CRITICA. This analysis confirms that there is no real discontinuity at length 100. Roughly 10{\%} of mouse proteins are shorter than 100 aa, although the majority of these are variants of proteins longer than 100 aa. We identify many novel short proteins, including a "dark matter" subset containing ones that lack detectable homology to other known proteins. Translation assays confirm that some of these novel proteins can be translated and localised to the secretory pathway.},
author = {Frith, Martin C. and Forrest, Alistair R. and Nourbakhsh, Ehsan and Pang, Ken C. and Kai, Chikatoshi and Kawai, Jun and Carninci, Piero and Hayashizaki, Yoshihide and Bailey, Timothy L. and Grimmond, Sean M.},
