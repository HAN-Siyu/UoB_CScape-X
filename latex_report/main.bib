journal = {Mol. BioSyst.},
number = {2},
pages = {656--663},
pmid = {25483728},
publisher = {Royal Society of Chemistry},
title = {{A global view of network of lncRNAs and their binding proteins}},
url = {http://xlink.rsc.org/?DOI=C4MB00409D},
volume = {11},
year = {2015}
}
