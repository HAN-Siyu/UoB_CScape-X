@article{Li2015a,
abstract = {BACKGROUND: Long noncoding RNAs (lncRNAs) have attracted significant attention in recent years due to their important roles in many biological processes. Domestic animals constitute a unique resource for understanding the genetic basis of phenotypic variation and are ideal models relevant to diverse areas of biomedical research. With improving sequencing technologies, numerous domestic-animal lncRNAs are now available. Thus, there is an immediate need for a database resource that can assist researchers to store, organize, analyze and visualize domestic-animal lncRNAs.{\$}\backslash{\$}n{\$}\backslash{\$}nRESULTS: The domestic-animal lncRNA database, named ALDB, is the first comprehensive database with a focus on the domestic-animal lncRNAs. It currently archives 12,103 pig intergenic lncRNAs (lincRNAs), 8,923 chicken lincRNAs and 8,250 cow lincRNAs. In addition to the annotations of lincRNAs, it offers related data that is not available yet in existing lncRNA databases (lncRNAdb and NONCODE), such as genome-wide expression profiles and animal quantitative trait loci (QTLs) of domestic animals. Moreover, a collection of interfaces and applications, such as the Basic Local Alignment Search Tool (BLAST), the Generic Genome Browser (GBrowse) and flexible search functionalities, are available to help users effectively explore, analyze and download data related to domestic-animal lncRNAs.{\$}\backslash{\$}n{\$}\backslash{\$}nCONCLUSIONS: ALDB enables the exploration and comparative analysis of lncRNAs in domestic animals. A user-friendly web interface, integrated information and tools make it valuable to researchers in their studies. ALDB is freely available from http://res.xaut.edu.cn/aldb/index.jsp.},
author = {Li, Aimin and Zhang, Junying and Zhou, Zhongyin and Wang, Lei and Liu, Yujuan and Liu, Yajun},
doi = {10.1371/journal.pone.0124003},
isbn = {1932-6203 (Electronic){\$}\backslash{\$}r1932-6203 (Linking)},
issn = {1932-6203},
journal = {Plos One},
number = {4},
pages = {e0124003},
pmid = {25853886},
title = {{ALDB: A Domestic-Animal Long Noncoding RNA Database}},
url = {http://dx.plos.org/10.1371/journal.pone.0124003},
volume = {10},
year = {2015}
}
